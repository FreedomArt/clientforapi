{
  "optimize-autoloader": true,
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-json": "*",
    "psr/simple-cache": ">=1.0",
    "psr/http-client": "~1.0",
    "guzzlehttp/psr7": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "phpfastcache/phpfastcache": "^7.1",
    "guzzlehttp/guzzle": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "ClientApi\\": "ClientApi/",
      "Http\\": "Http/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ClientApi\\Tests\\": "tests/"
    }
  }
}